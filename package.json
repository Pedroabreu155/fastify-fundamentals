{
  "name": "fastify-learning-project",
  "version": "1.0.0",
  "description": "Project to learn about fastify",
  "main": "app.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "check": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "knex": "node --import tsx ./node_modules/.bin/knex",
    "test": "vitest run --dir ./tests/units",
    "test:e2e": "vitest run --dir ./tests/e2e"
  },
  "keywords": [],
  "author": "Pedroabreu155",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.17",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.1.0",
    "supertest": "^7.1.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.3",
    "vitest-tsconfig-paths": "^3.4.1"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.2",
    "knex": "^3.1.0",
    "zod": "^3.24.4"
  }
}
